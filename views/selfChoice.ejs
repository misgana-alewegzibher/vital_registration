    <section class="choice">
      <h1>UserId: <%= usrId %></h1>

      
      <form action="birthChoice/<%=usrId%>">
        
        <div class="container-md py-5 h-100">
          <div
            class="row d-flex justify-content-center align-items-center h-100"
          >
            <div class="col-lg-6  col-md-6 col-sm-3 col-xs-3 mb-5">
              <!-- clickable image for birth  -->
              <div class="card bg-dark text-white" style="border-radius: 1rem">
                <div class="card-body p-5 text-center">
                  <div class="mb-md-5 mt-md-4 pb-5">
                    <button
                      type="submit"
                      id="birthbtn"
                      class="border-0 rounded-4"
                    >
                      <img
                        class="img-fluid"
                        src="../images/services/Hospital family visit-amico.png"
                        alt="birth image "
                      />
                    </button>
                    <p class="display-2--description m-3"> CLICK HERE TO ENTER BIRTH INFORMATION </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        </form>
